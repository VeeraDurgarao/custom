<odoo>
    <data>
       <record id="view_commission_id" model="ir.ui.view">
    <field name="name">commission_model.form.views</field>
    <field name="model">commission.model</field>
    <field name="arch" type="xml">
        <form string="Loan">
            <header>
                <button name="action_show_matching_orders" type="object" string="List"/>
                   <button name="generates_excel_report" string="Print Excel" type="object" class="oe_highlight"/>
<!--            <button name="get_custom_data" type="object" string="SQL"/>-->
            </header>
            <group>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="customer"/>
            </group>
            <notebook>
                <page string="Matching Order List">
                    <field name="list"/>
                </page>
            </notebook>
        </form>
    </field>
</record>

          <record id="views_commission_id" model="ir.ui.view">
    <field name="name">commission_model.tree.views</field>
    <field name="model">commission.model</field>
    <field name="arch" type="xml">
                <tree string="commission">
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="customer"/>
                    <field name="list" widget="many2many_tags"/>
                </tree>
            </field>
        </record>
          <record id="view_sale_order_form_inherits" model="ir.ui.view">
       <field name="name">sale.order</field>
       <field name="model">sale.order</field>
       <field name="inherit_id" ref="sale.view_order_form"/>
       <field name="arch" type="xml">
           <xpath expr="//form" position="attributes">
               <attribute name="js_class">model_info</attribute>
           </xpath>
       </field>
   </record>

          <record id="view_stock_order_form_inherits" model="ir.ui.view">
       <field name="name">stock.picking</field>
       <field name="model">stock.picking</field>
       <field name="inherit_id" ref="stock.view_picking_form"/>
       <field name="arch" type="xml">
           <xpath expr="//form" position="attributes">
               <attribute name="js_class">stock_info</attribute>
           </xpath>
       </field>
   </record>
        <record id="view_tree_inherit" model="ir.ui.view">
        <field name="name">sale.order</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.sale_order_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">model_sale</attribute>
            </xpath>
        </field>
    </record>

        <record id="view_trees_inherit" model="ir.ui.view">
        <field name="name">bank.account</field>
        <field name="model">bank.account</field>
        <field name="inherit_id" ref="bank.view_bank_bank_account"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">add_new</attribute>
            </xpath>
        </field>
    </record>
             <record id="view_trees_inherits" model="ir.ui.view">
        <field name="name">product.template</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">manifest_but</attribute>
            </xpath>
        </field>
    </record>

        <record id="view_hr_exre_tree_inherit" model="ir.ui.view">
        <field name="name">hr.expense</field>
        <field name="model">hr.expense</field>
        <field name="inherit_id" ref="hr_expense.hr_expense_view_expenses_analysis_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">model_hr</attribute>
            </xpath>
        </field>
    </record>

        <record id="sale_order_commission_action_id" model="ir.actions.act_window">
            <field name="name">sale LIST</field>
            <field name="res_model">commission.model</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>
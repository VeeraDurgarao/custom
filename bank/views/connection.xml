        <record id="connections_product_action_id" model="ir.actions.act_window">
            <field name="name">Connection</field>
            <field name="type">ir.actions.act_window</field>
             <field name="res_model">product.template</field>
            <field name="view_mode">kanban,tree,form</field>
        </record>

        <menuitem id="menu_connection_master"
              name="Customers"
              parent="menu_practice_master"
              action="connection_connection_action_id"
              />
        <menuitem id="menu_order_master"

         <record id="res_config_settings_view_form" model="ir.ui.view">
       <field name="name">
           res.config.settings.view.form.inherit.point_of_sales
       </field>
       <field name="model">res.config.settings</field>
       <field name="inherit_id"
              ref="point_of_sale.res_config_settings_view_form"/>
       <field name="arch" type="xml">
           <xpath expr="//setting[@id='variant_options']"
                  position="before">
               <setting id="sale_discount_limit"
            title="User can limit the discount amount in the
                    sale order line."

                 help="User can limit the discount amount">
<!--                   invisible="not group_discount_per_so_line"-->
                   <field name="is_discount_limit"/>
                   <div class="content-group"
                        >
<!--                       invisible="not is_discount_limit"-->
                       <div class="mt16">
                           <label for="discount_limit"/>
                           <field name="discount_limit"/>
                       </div>
                   </div>
               </setting>
           </xpath>
       </field>
   </record>

    <record id="res_config_settings_views_form_my" model="ir.ui.view">
        <field name="name">res.config.settings.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='pos_payment_method_ids']" position="after">
                <group>
                <field name="custom_field" />
                </group>
            </xpath>
        </field>
    </record>



        <record id="res_config_settings_views_form_percentage" model="ir.ui.view">
        <field name="name">res.config.settings.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//block[@id='restaurant_section']" position="after">
                     <div>
                        <block title="PointOfSale Percentage">
                            <setting>
                                 <div>
                                     <group>
                                  <field name="Percentage"/>
                                  <field name="location" widget="many2many_tags"/>
                                </group>
                                 </div>
<!--                                <div class="content-group" id="warning_text_pos_percentage__restaurant">-->
<!--                                    <div class="text-warning ">-->
<!--                                        Save this page and come back here to set up the feature.-->
<!--                                    </div>-->
<!--                                </div>-->
                            </setting>
                        </block>
                     </div>
            </xpath>
        </field>
    </record>




        <record id="connection_connection_action_id" model="ir.actions.act_window">
            <field name="name">Connection</field>
            <field name="type">ir.actions.act_window</field>
<!--            <field name="res_model">connection.order</field>-->
             <field name="res_model">res.partner</field>
<!--            <field name='view_id' ref="base.res_partner_kanban_view"></field>-->
<!--            <field name="domain">[('customer_rank','&gt;', 0)]</field>-->
            <field name="view_mode">kanban,form</field>
        </record>
         <record id="connections_order_action_id" model="ir.actions.act_window">
            <field name="name">Connection</field>
            <field name="type">ir.actions.act_window</field>
<!--            <field name="res_model">connection.order</field>-->
             <field name="res_model">sale.order</field>
<!--            <field name='view_id' ref="base.res_partner_kanban_view"></field>-->
<!--            <field name="domain">[('customer_rank','&gt;', 0)]</field>-->

<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="bank.expensebtn" t-inherit="planning.PlanningGanttView.Buttons"  t-inherit-mode="extension">
        <xpath expr="//button[@t-if='model.metaData.canCreate']" position="after">
            <button class="btn btn-secondary" t-on-click="actionPlanning">
                Button
            </button>
        </xpath>
    </t>

    <t t-name="bank.iconsAdding" t-inherit="website.ThemePreviewForm.ControlPanel" t-inherit-mode="extension">
        <xpath expr="//div[contains(@class,'o_control_panel_navigation')]" position="after">
            <div>
                <button class="btn btn-secondary">
                    <i class="fa fa-man" />
                </button>
            </div>
        </xpath>
    </t>


</templates>
